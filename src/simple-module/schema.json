{
  "$schema": "http://json-schema.org/schema",
  "id": "SchematicsAngularDimpleModule",
  "title": "Angular simple module",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "The name of the module.",
      "$default": {
        "$source": "argv",
        "index": 0
      }
    },
    "project": {
      "type": "string",
      "description": "The name of the project.",
      "$default": {
        "$source": "projectName"
      }
    },
    "namespace":  {
      "type": "string",
      "description": "Allows specification of the declaring module.",
      "alias": "n"
    },
    "entity":  {
      "type": "string",
      "description": "Allows specification of the declaring module.",
      "alias": "e"
    },
    "id": {
      "type": "string",
      "description": "The name of the entiry id.",
      "$default": {
        "$source": "argv",
        "index": 1
      }
    }
  },
  "required": []
}
